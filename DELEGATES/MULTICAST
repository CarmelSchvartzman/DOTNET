public delegate int d(int k);

 public int F(int k) { return 0; }
        public int F1(int k) { return 1; }
        public int F2(int k) { return 2; }
        
///////  USE :

 d i = new d(F);
            i  += F1;
            i += F2;
            int res = i(0);  //  <<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<
            
 
//////// RESULTS :

// res = 2     //  <<<<<<<<<<<<<<<<<<<  ONLY LAST METHOD RETURN DATA
